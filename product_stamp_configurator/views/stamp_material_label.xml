<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="stamp_material_label_view_tree" model="ir.ui.view">
        <field name="name">stamp.material.label.tree</field>
        <field name="model">stamp.material.label</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="stamp_material_label_view_form" model="ir.ui.view">
        <field name="name">stamp.material.label.form</field>
        <field name="model">stamp.material.label</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group name="main">
                        <group name="left">
                            <field name="name"/>
                        </group>
                        <group name="right"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="stamp_material_label_action" model="ir.actions.act_window">
        <field name="name">Material Labels</field>
        <field name="res_model">stamp.material.label</field>
        <field name="view_mode">tree,form</field>
    </record>

</odoo>
